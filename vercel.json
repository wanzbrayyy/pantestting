{
  "version": 2,
  "functions": {
    "api/**/*": {
      "excludeFiles": "{node_modules,*.cache}/**"
    }
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["**/*"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
